<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AFD3B0"
    tools:context=".StoreOwnerApp.Account.OwnAccountFragment">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/own_home_empty_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/own_home_supermarket_icon"
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:contentDescription="@string/empty"
                android:src="@drawable/supermarket"
                app:layout_constraintBottom_toTopOf="@+id/own_home_add_supermarket"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.919" />

            <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
                android:id="@+id/own_home_add_supermarket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_your_supermarket"
                android:textColor="@color/white"
                android:theme="@style/Theme.MaterialComponents.NoActionBar"
                app:backgroundTint="#8BC34A"
                app:icon="@drawable/ic_add"
                app:iconTint="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>